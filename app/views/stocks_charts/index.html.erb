<p id="notice"><%= notice %></p>

<h1>Stocks Charts</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Open</th>
      <th>High</th>
      <th>Low</th>
      <th>Close</th>
      <th>Volume</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stocks_charts.each do |stocks_chart| %>
      <tr>
        <td><%= stocks_chart.date %></td>
        <td><%= stocks_chart.open %></td>
        <td><%= stocks_chart.high %></td>
        <td><%= stocks_chart.low %></td>
        <td><%= stocks_chart.close %></td>
        <td><%= stocks_chart.volume %></td>
        <td><%= stocks_chart.value %></td>
        <td><%= link_to 'Show', stocks_chart %></td>
        <td><%= link_to 'Edit', edit_stocks_chart_path(stocks_chart) %></td>
        <td><%= link_to 'Destroy', stocks_chart, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stocks Chart', new_stocks_chart_path %>
